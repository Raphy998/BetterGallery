{
    "manifest_version": 2,
    "name": "BetterGallery",
    "version": "1.0",
    "author": "Raphy998",

    "description": "Adds an better image gallery to pages",

    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },

    "content_scripts": [
        {
            "matches": ["*://*.kleinezeitung.at/*"],
            "js": ["contentScripts/kleine.js"]
        }
    ]
}